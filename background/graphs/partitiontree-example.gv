graph
{
	node [shape=ellipse, style=solid, color=black, label=""];

	// Nodes
	n0a [label="{a,b,c,d,e,f}", width=2.5];
	n1a [label="{a,b}"];
	n1c [label="{c,d,e}"];
	n1f [label="{f}"];
	n2a [label="{a}"];
	n2b [label="{b}"];
	n2c [label="{c}"];
	n2d [label="{d,e}"];
	n3b [label="{b}"];
	n3d [label="{d}"];
	n3e [label="{e}"];

	// Edges
	n0a -- n1a;
	n0a -- n1c;
	n0a -- n1f;
	n1a -- n2a;
	n1a -- n2b;
	n1c -- n2c;
	n1c -- n2d;
	n2b -- n3b;
	n2d -- n3d;
	n2d -- n3e;
}
